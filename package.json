{
    "name": "relayer-ui",
    "version": "1.0.0",
    "description": "XDC Relayer UI",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hash-laboratories-au/relayer-ui.git"
    },
    "license": "MIT",
    "homepage": "https://hash-laboratories-au.github.io/relayer-ui/",
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.1",
        "@types/node": "^16.11.36",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@types/react-router": "^5.1.18",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "css-loader": "^6.7.1",
        "postcss": "^8.2.8",
        "postcss-loader": "^7.0.0",
        "postcss-preset-env": "^7.6.0",
        "copy-webpack-plugin": "^11.0.0",
        "autoprefixer": "10.2.5",
        "dotenv-webpack": "^7.1.0",
        "eslint": "^8.16.0",
        "eslint-plugin-react": "^7.30.0",
        "gh-pages": "^4.0.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "react-scripts": "5.0.1",
        "ts-loader": "^9.3.0",
        "typescript": "^4.7.3",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.0",
        "patch-package": "^6.5.1"
    },
    "dependencies": {
        "antd": "^5.3.3",
        "axios": "^1.3.4",
        "moment": "^2.29.3",
        "react": "18.1.0",
        "tailwindcss": "3.0.24",
        "react-d3-tree": "^3.3.2",
        "react-dom": "18.1.0",
        "react-router": "6.3.0",
        "web3": "^1.8.2",
        "react-router-dom": "6.3.0"
    },
    "scripts": {
        "dev": "npm run build:tailwind && npx webpack serve --mode development",
        "build:dev": "npx webpack --mode=development",
        "build:prod": "npx webpack --mode=production --node-env=production",
        "build:tailwind": "npx tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
        "install:clean": "rm -rf node_modules && npm install && npm run build:tailwind && npm run build:prod",
        "predeploy": "rm -rf dist build && npm run build:tailwind && npm run build:prod",
        "deploy:github": "gh-pages -d build",
        "start": "./bin/start.sh",
        "postinstall": "patch-package"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "bugs": {
        "url": "https://github.com/hash-laboratories-au/relayer-ui/issues"
    },
    "main": "webpack.config.js",
    "author": ""
}